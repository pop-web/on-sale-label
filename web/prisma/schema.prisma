// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model label {
  id           Int     @id @default(autoincrement())
  title        String
  productId    String
  variantId    String
  handle       String
  destination  String?
  discountId   String?
  discountCode String?
}
